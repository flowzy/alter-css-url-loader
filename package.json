{
  "name": "alter-css-url-loader",
  "version": "1.0.4",
  "description": "Webpack loader for a more custom CSS url() altering",
  "author": "flowzy <github.com/flowzy>",
  "bugs": {
    "url": "https://github.com/flowzy/alter-css-url-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowzy/alter-css-url-loader.git"
  },
  "homepage": "https://github.com/flowzy/alter-css-url-loader#readme",
  "license": "MIT",
  "main": "dist/loader.js",
  "scripts": {
    "clean": "del-cli dist",
    "compile": "cross-env NODE_ENV=production babel src -d dist --extensions '.ts' --no-comments --copy-files --ignore src/typings && del-cli dist/typings",
    "build": "npm run clean && npm run compile",
    "test": "jest --detectOpenHandles --config .jestrc.js",
    "coveralls": "npm run test --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "keywords": [
    "webpack",
    "loader",
    "css",
    "url",
    "replace",
    "webpack-loader",
    "sass",
    "css-url-loader",
    "reddit"
  ],
  "dependencies": {
    "loader-utils": "^1.2.3",
    "schema-utils": "^2.6.1",
    "sprintf-js": "^1.1.2"
  },
  "peerDependencies": {
    "webpack": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/jest": "^24.0.25",
    "@types/loader-utils": "^1.1.3",
    "@types/memory-fs": "^0.3.2",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-jest": "^24.9.0",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  }
}
