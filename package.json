{
  "name": "alter-css-url-loader",
  "version": "1.1.0",
  "description": "Webpack loader for a more custom CSS url() altering",
  "author": "flowzy <github.com/flowzy>",
  "bugs": {
    "url": "https://github.com/flowzy/alter-css-url-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowzy/alter-css-url-loader.git"
  },
  "homepage": "https://github.com/flowzy/alter-css-url-loader#readme",
  "license": "MIT",
  "main": "dist/loader.js",
  "scripts": {
    "prepare": "install-peers",
    "clean": "del-cli dist",
    "compile": "cross-env NODE_ENV=production babel src -d dist --extensions '.ts' --no-comments --copy-files",
    "build": "npm run clean && npm run compile",
    "test": "jest --detectOpenHandles --config .jestrc.js",
    "coveralls": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint './{src,test,types}/**/*.{js,ts}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 8.12.0"
  },
  "keywords": [
    "webpack",
    "loader",
    "css",
    "url",
    "replace",
    "webpack-loader",
    "sass",
    "css-url-loader",
    "reddit"
  ],
  "dependencies": {
    "loader-utils": "^2.0.0",
    "memfs": "^3.2.2",
    "schema-utils": "^3.0.0",
    "sprintf-js": "^1.1.2",
    "webpack": "^5.0.0"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/jest": "^26.0.23",
    "@types/loader-utils": "^2.0.2",
    "@types/memory-fs": "^0.3.2",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "del-cli": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^6.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  }
}
