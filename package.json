{
  "name": "alter-css-url-loader",
  "version": "1.0.2",
  "description": "Webpack loader for a more custom CSS url() altering",
  "author": "flowzy <github.com/flowzy>",
  "bugs": {
    "url": "https://github.com/flowzy/alter-css-url-loader/issues"
  },
  "homepage": "https://github.com/flowzy/alter-css-url-loader#readme",
  "license": "MIT",
  "main": "dist/loader.js",
  "scripts": {
    "clean": "del-cli dist",
    "compile": "cross-env NODE_ENV=production babel src -d dist --copy-files --presets minify --no-comments",
    "build": "npm run clean && npm run compile",
    "test": "jest --detectOpenHandles",
    "coveralls": "jest --detectOpenHandles --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowzy/alter-css-url-loader.git"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "keywords": [
    "webpack",
    "loader",
    "css",
    "url",
    "replace",
    "webpack-loader",
    "sass",
    "css-url-loader",
    "reddit"
  ],
  "dependencies": {
    "loader-utils": "^1.2.3",
    "schema-utils": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "del-cli": "^3.0.0",
    "jest": "^24.9.0",
    "webpack": "^4.41.5"
  }
}
